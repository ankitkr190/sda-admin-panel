{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "name": "Students"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Email": {
      "type": "email"
    },
    "Phone": {
      "type": "integer"
    },
    "DOB": {
      "type": "date"
    },
    "Photo": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "Aadhar": {
      "type": "biginteger"
    },
    "RegistrationNo": {
      "type": "integer"
    },
    "Category": {
      "type": "enumeration",
      "enum": [
        "Gym",
        "Taekwondo",
        "Skating",
        "Dance"
      ]
    },
    "Membership": {
      "type": "string"
    }
  }
}
